h2
{
	font-size: 1.2em;
	text-align: center;
	text-transform: uppercase;
}
h2:before
{
	content: "«  ";
	opacity: 0;
	position: relative;
	right: -0.5em;
	transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-webkit-transition: all 0.3s ease-in-out;
	visibility: hidden;
}
h2:hover:before
{
	opacity: 1;
	right: 0;
	visibility: visible;
}
h2:after, h3:after, h4:after
{
	content: "  »";
	left: -0.5em;
	opacity: 0;
	position: relative;
	transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-webkit-transition: all 0.3s ease-in-out;	
	visibility: hidden;
}
h2:hover:after, h2:hover:after, h3:hover:after, h4:hover:after
{
	left: 0;
	opacity: 1;
	visibility: visible;
}
