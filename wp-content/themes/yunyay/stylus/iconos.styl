.footer
	padding-bottom 6rem

.iconos
	// background-color $blanco_anaranjado
	bottom 0
	left 0
	position fixed
	width 100%
	padding 0.2rem

	ul
		list-style none
		padding 0
		margin 0

		li
			display inline-block
			padding 0
			margin 0

			a
				font-size 2rem
				padding 0.4rem
				margin 0 0.2rem

#irarriba
	background rgba(0,0,0,0.5)
	bottom 2.5rem
	font-size 1.5rem
	display inline-block
	padding 0.3rem
	position fixed
	right 0.2em
	z-index 999
	// height 40px

	&:hover
		background $naranja !important

@media all and (min-width: 460px)
	.footer
		padding-bottom 4rem

	#irarriba
		bottom 0.5rem

@media all and (min-width: 960px)
	.footer
		padding-bottom 1rem

	.iconos
		padding-top 4rem
		position static