.list_item
{
	// background: rgba(0,0,0,0.5);
	box-sizing: border-box;
	padding: 0.1em 0.5em 0.5em 0.5em;
	margin: 0 0.5em 1em 0.5em;
}
.list_item.contacto
{
	padding: 1em 0;
}
.list_item div.list_item_img
{
	margin-top: 1em;
}
.list_item figure img
{
	border-radius: 0.5em;
}
.list_item figure figcaption
{	
	background: #DC5B34;
	border-radius: 0 0 0.5em 0.5em;
	color: #efefef;
	margin-top: -0.4em;
	text-shadow: 0px -1px 0px $negro_oscuro_transparente;
	transition: all ease 0.4s;
}
.list_item h2
{
	color: orange;
	padding: 0.2em 0.2em 0.2em 0.5em;
	margin: 1em auto;
}
.navegacion
{
	display: block;
	font-family: 'Raleway';
	font-weight: lighter;
	position: absolute;
	top: 35%;
	width: 100%;
	z-index: 120;
}
.navegacion div
{
	display: inline-block;
}
.navegacion div a
{
	background: rgba(0,0,0,0.3);
	color: #eee;
	cursor: pointer;
	font-size: 3em;
	padding: 0 0.3em 0.2em 0.3em;
}
.navegacion div a:hover
{
	color: #E39250;
}
.navegacion div.back
{
	float: left;
}
.navegacion div.back a
{
	border-radius: 0 0.2em 0.2em 0;
	border-right: 4px solid rgba(0,0,0,0.5);
	border-top: 2px solid rgba(0,0,0,0.5);
}
.navegacion div.next
{
	float: right;
}
.navegacion div.next a
{
	border-left: 4px solid rgba(0,0,0,0.5);
	border-radius: 0.2em 0 0 0.2em;
	border-top: 2px solid rgba(0,0,0,0.5);
}
.pagination
{
	color: orange;
	display: block;
	margin: 0.5em;
	padding: 0;
}
.pagination a, .pagination li
{
	background: rgba(0,0,0,0.5);
	display: inline;
	margin: 0 1px;
	padding: 0.3em 0.5em;
	text-decoration: none;
}
.pagination a
{
	color: #ddd;
	font-weight: bolder;
	text-decoration: underline;
	transition: all 0.4s ease;
}
.pagination a:hover, .pagination a:active, .pagination a:focus
{
	background: rgba(230,220,240,0.9);
	color: #003333;
}
.pagination .current
{
	background: rgba(0,0,0,0.5);
	color: orange;
	padding: 0.3em 0.5em;
}
.redes_sociales
{
	padding: 0.5em 0 0 0;
}
.servicios div ul
{
	list-style: none;
	padding: 0;
}
.servicios figcaption
{
	background: rgba(22, 107, 236,0.8);
	bottom: 0;
	color: #ededed;
	display: block;
	font-family: 'Open Sans';
	letter-spacing: 2px;
	padding: 0.3em;
	position: absolute;
	text-align: center;
	text-transform: uppercase;
	width: 100%;
}
.servicios figure
{
	margin: 0;
	overflow: hidden;
	padding: 0;
	position: relative;
	max-width: 300px;
}
.servicios figure img
{
	margin: 0;
	padding: 0;
	width: 100%;
}
.servicios ul li
{
	display: inline-block;
	padding: 0;
	margin: -2px -2px;
}
.separador
	display none
