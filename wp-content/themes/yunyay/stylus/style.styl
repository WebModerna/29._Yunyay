	{
		display: block;
		margin-left: 0.3em;
		padding: 0;
		transform: rotate(-3deg);
		z-index: 0;
	}
	.servicios ul li
	{
		transform: scale(1.1);
	}
	.servicios .contenido
	{
		margin: auto 1em 1em auto;
		position: relative;
		text-align: right;
		width: 70%;
	}
}

